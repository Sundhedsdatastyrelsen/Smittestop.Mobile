<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:id="@+id/infection_status_main_layout">
	<ImageButton
			android:layout_width="32dp"
			android:layout_height="32dp"
			android:layout_alignParentEnd="true"
			android:layout_alignParentTop="true"
			android:layout_marginTop="20dp"
			android:layout_marginEnd="16dp"
			android:background="@drawable/ic_settings"
			android:id="@+id/infection_status_menu_icon_relativeLayout" />
	<ImageView
			android:src="@drawable/ic_logo_no_chain"
			android:layout_width="wrap_content"
			android:layout_height="40dp"
			android:layout_alignParentTop="true"
			android:layout_centerHorizontal="true"
			android:layout_marginTop="20dp"
			android:padding="5dp"
			android:id="@+id/infection_status_app_icon_imageView" />
	<ScrollView
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_below="@+id/infection_status_app_icon_imageView"
			android:id="@+id/infection_status_scrollView">
		<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingTop="54dp"
				android:paddingBottom="54dp"
				android:id="@+id/infection_status_relativeLayout">
			<TextView
					android:text="Text"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="10dp"
					android:layout_centerHorizontal="true"
					android:gravity="center"
					android:id="@+id/infection_status_activity_status_textView"
					style="@style/PrimaryTextBold" />
			<RelativeLayout
					android:layout_width="140dp"
					android:layout_height="140dp"
					android:layout_centerHorizontal="true"
					android:layout_below="@+id/infection_status_activity_status_textView"
					android:id="@+id/infection_status_circle_background">
				<ImageView
						android:background="@drawable/background_circle_green"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerHorizontal="true"
						android:layout_centerVertical="true"
						android:id="@+id/infection_status_background" />
				<ImageButton
						android:src="@drawable/ic_pause"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerHorizontal="true"
						android:layout_centerVertical="true"
						android:id="@+id/infection_status_on_off_button"
						style="@style/InfectionStatusOnOffButtonGreen" />

			</RelativeLayout>
			<TextView
					android:text="Text"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_marginTop="10dp"
					android:layout_marginBottom="10dp"
					android:layout_marginRight="36dp"
					android:layout_marginLeft="36dp"
					android:layout_below="@+id/infection_status_circle_background"
					android:gravity="center"
					android:id="@+id/infection_status_activivity_status_description_textView"
					style="@style/PrimaryTextRegular" />
			<RelativeLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:importantForAccessibility="no"
					android:layout_below="@+id/infection_status_activivity_status_description_textView"
					android:id="@+id/infection_status_disease_rate_button_relativeLayout">
				<RelativeLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:importantForAccessibility="no"
						android:id="@+id/infection_status_disease_rate_button_relativeLayout_inner"
						style="@style/InfectionStatusLayoutButton">
					<ImageView
							android:src="@drawable/ic_disease_rate"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_alignParentStart="true"
							android:layout_marginEnd="21dp"
							android:id="@+id/infection_status_disease_rate_imageView"/>
					<TextView
							android:text="Text"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginBottom="4dp"
							android:importantForAccessibility="no"
							android:breakStrategy="balanced"
							android:layout_toEndOf="@+id/infection_status_disease_rate_imageView"
							android:layout_toStartOf="@+id/infection_status_disease_rate_arrow_imageView"
							android:id="@+id/infection_status_disease_rate_text_textView"
							style="@style/PrimaryTextBold" />
					<TextView
							android:text="Text"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:importantForAccessibility="no"
							android:breakStrategy="balanced"
							android:layout_marginEnd="8dp"
							android:layout_toEndOf="@+id/infection_status_disease_rate_imageView"
							android:layout_toStartOf="@+id/infection_status_disease_rate_arrow_imageView"
							android:layout_below="@+id/infection_status_disease_rate_text_textView"
							android:id="@+id/infection_status_new_disease_rate_text_textView"
							style="@style/PrimaryTextRegular" />
					<ImageView
							android:src="@drawable/ic_arrow_back_right"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_alignParentEnd="true"
							android:layout_marginStart="5dp"
							android:paddingStart="20dp"
							android:paddingEnd="0dp"
							android:id="@+id/infection_status_disease_rate_arrow_imageView" />
				</RelativeLayout>
				<Button
						android:id="@+id/infection_status_disease_rate_button_relativeLayout_button"
						android:importantForAccessibility="yes"
						android:background="@android:color/transparent"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:layout_alignTop="@+id/infection_status_disease_rate_button_relativeLayout_inner"
						android:layout_alignBottom="@+id/infection_status_disease_rate_button_relativeLayout_inner"
						android:layout_alignStart="@+id/infection_status_disease_rate_button_relativeLayout_inner"
						android:layout_alignEnd="@+id/infection_status_disease_rate_button_relativeLayout_inner"
						android:minHeight="0dp"
						android:minWidth="0dp" />
			</RelativeLayout>
			<RelativeLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:importantForAccessibility="no"
					android:layout_below="@+id/infection_status_disease_rate_button_relativeLayout"
					android:id="@+id/infection_status_messages_button_relativeLayout">
				<RelativeLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:importantForAccessibility="no"
						android:id="@+id/infection_status_messages_button_relativeLayout_inner"
						style="@style/InfectionStatusLayoutButton">
					<ImageView
							android:src="@drawable/ic_notification"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_alignParentStart="true"
							android:layout_marginEnd="21dp"
							android:id="@+id/infection_status_message_bell_imageView" />
					<TextView
							android:text="Text"
							android:layout_width="0dp"
							android:layout_height="wrap_content"
							android:layout_marginBottom="4dp"
							android:importantForAccessibility="no"
							android:breakStrategy="balanced"
							android:layout_toEndOf="@+id/infection_status_message_bell_imageView"
							android:layout_toStartOf="@+id/infection_status_message_arrow_imageView"
							android:id="@+id/infection_status_message_text_textView"
							style="@style/PrimaryTextBold" />
					<ImageView
							android:src="@drawable/ic_notification_dot"
							android:layout_width="10dp"
							android:layout_height="10dp"
							android:layout_marginEnd="8dp"
							android:layout_toEndOf="@+id/infection_status_message_bell_imageView"
							android:layout_below="@+id/infection_status_message_text_textView"
							android:layout_alignTop="@+id/infection_status_new_message_text_textView"
							android:layout_alignBottom="@+id/infection_status_new_message_text_textView"
							android:id="@+id/infection_status_message_new_message_imageView" />
					<TextView
							android:text="Text"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:importantForAccessibility="no"
							android:breakStrategy="balanced"
							android:layout_toEndOf="@+id/infection_status_message_new_message_imageView"
							android:layout_toStartOf="@+id/infection_status_message_arrow_imageView"
							android:layout_below="@+id/infection_status_message_text_textView"
							android:id="@+id/infection_status_new_message_text_textView"
							style="@style/PrimaryTextRegular" />
					<ImageView
							android:src="@drawable/ic_arrow_back_right"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_alignParentEnd="true"
							android:layout_marginStart="5dp"
							android:paddingStart="20dp"
							android:paddingEnd="0dp"
							android:id="@+id/infection_status_message_arrow_imageView" />
				</RelativeLayout>
				<Button
						android:id="@+id/infection_status_messages_button_relativeLayout_button"
						android:importantForAccessibility="yes"
						android:background="@android:color/transparent"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:layout_alignTop="@+id/infection_status_messages_button_relativeLayout_inner"
						android:layout_alignBottom="@+id/infection_status_messages_button_relativeLayout_inner"
						android:layout_alignStart="@+id/infection_status_messages_button_relativeLayout_inner"
						android:layout_alignEnd="@+id/infection_status_messages_button_relativeLayout_inner"
						android:minHeight="0dp"
						android:minWidth="0dp" />
			</RelativeLayout>
			<RelativeLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:importantForAccessibility="no"
					android:layout_below="@+id/infection_status_messages_button_relativeLayout"
					android:id="@+id/infection_status_registration_button_relativeLayout">
				<RelativeLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:importantForAccessibility="no"
						android:id="@+id/infection_status_registration_button_relativeLayout_inner"
						style="@style/InfectionStatusLayoutButton">
					<ImageView
							android:src="@drawable/ic_covid_virus"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:minWidth="25dp"
							android:minHeight="25dp"
							android:layout_centerVertical="true"
							android:layout_alignParentStart="true"
							android:layout_marginEnd="21dp"
							android:id="@+id/infection_status_registration_virus_imageView" />
					<TextView
							android:text="Text"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginBottom="4dp"
							android:importantForAccessibility="no"
							android:breakStrategy="balanced"
							android:layout_toEndOf="@+id/infection_status_registration_virus_imageView"
							android:layout_toStartOf="@+id/infection_status_registration_arrow_imageView"
							android:id="@+id/infection_status_registration_text_textView"
							style="@style/PrimaryTextBold" />
					<TextView
							android:text="Text"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:importantForAccessibility="no"
							android:breakStrategy="balanced"
							android:layout_marginEnd="8dp"
							android:layout_toEndOf="@+id/infection_status_registration_virus_imageView"
							android:layout_toStartOf="@+id/infection_status_registration_arrow_imageView"
							android:layout_below="@+id/infection_status_registration_text_textView"
							android:id="@+id/infection_status_registration_login_text_textView"
							style="@style/PrimaryTextRegular" />
					<ImageView
							android:src="@drawable/ic_arrow_back_right"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_alignParentEnd="true"
							android:layout_marginStart="5dp"
							android:paddingStart="20dp"
							android:paddingEnd="0dp"
							android:id="@+id/infection_status_registration_arrow_imageView" />
				</RelativeLayout>
				<Button
						android:id="@+id/infection_status_registration_button_relativeLayout_button"
						android:importantForAccessibility="yes"
						android:background="@android:color/transparent"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:layout_alignTop="@+id/infection_status_registration_button_relativeLayout_inner"
						android:layout_alignBottom="@+id/infection_status_registration_button_relativeLayout_inner"
						android:layout_alignStart="@+id/infection_status_registration_button_relativeLayout_inner"
						android:layout_alignEnd="@+id/infection_status_registration_button_relativeLayout_inner"
						android:minHeight="0dp"
						android:minWidth="0dp" />
			</RelativeLayout>
		</RelativeLayout>
	</ScrollView>
	<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="wrap_content"
			android:layout_height="30dp"
			android:layout_alignParentBottom="true"
			android:layout_centerInParent="true"
			android:layout_marginBottom="13dp"
			android:paddingStart="20dp"
			android:paddingEnd="20dp"
			android:orientation="vertical"
			android:background="@drawable/infection_status_scroll_down_background"
			android:id="@+id/infection_status_scroll_down_layout"
			android:visibility="gone"
			tools:visibility="visible">
		<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:orientation="horizontal"
				android:gravity="end">
			<ImageView
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:src="@drawable/ic_down_arrow"
					android:id="@+id/infection_status_down_arrow"
					android:gravity="center_vertical"/>
			<TextView
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:text="Text"
					android:layout_marginLeft="8dp"
					android:id="@+id/infection_status_down_text"
					android:gravity="center_vertical"
					android:textColor="@color/counterLayoutBackgroundColor"/>
		</LinearLayout>
	</androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>