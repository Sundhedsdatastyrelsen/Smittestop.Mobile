<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

    <include
            android:id="@+id/consent_paragraph_frivillig_brug"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp" />

    <include
            android:id="@+id/consent_paragraph_sadan_fungerer_appen"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consent_paragraph_frivillig_brug"
            android:layout_marginBottom="16dp" />

    <include
            android:id="@+id/consent_paragraph_hvad_registreres"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consent_paragraph_sadan_fungerer_appen"
            android:layout_marginBottom="16dp" />

    <include
            android:id="@+id/consent_paragraph_hvordan_accepterer"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consent_paragraph_hvad_registreres"
            android:layout_marginBottom="16dp" />

    <include
            android:id="@+id/consent_paragraph_ret"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consent_paragraph_hvordan_accepterer"
            android:layout_marginBottom="16dp" />

    <include
            android:id="@+id/consent_paragraph_kontaktregistringer"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consent_paragraph_ret"
            android:layout_marginBottom="16dp" />

    <include
            android:id="@+id/consent_paragraph_mere"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consent_paragraph_kontaktregistringer"
            android:layout_marginBottom="16dp" />

    <Button
            android:id="@+id/consent_paragraph_policy_btn"
            style="@style/ConsentButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/consent_paragraph_mere"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="16dp"
            android:text="Privacy policy" />

    <include
            android:id="@+id/consent_paragraph_behandlingen"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/consent_paragraph_policy_btn"
            android:layout_marginBottom="16dp" />

    <include
            android:id="@+id/consent_paragraph_aendringer"
            layout="@layout/consent_paragraph"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/consent_paragraph_behandlingen"
            android:layout_marginBottom="16dp" />
</RelativeLayout>