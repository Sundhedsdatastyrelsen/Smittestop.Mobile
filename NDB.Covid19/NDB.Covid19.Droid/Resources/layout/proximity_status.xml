<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<RelativeLayout
		android:id="@+id/icon_settin"
		android:layout_width="32dp"
		android:layout_height="32dp"
		android:layout_alignParentEnd="true"
		android:layout_alignParentTop="true"
		android:layout_marginTop="20dp"
		android:layout_marginEnd="16dp"
		android:background="@drawable/ic_settings" />
	<ImageView
		android:src="@drawable/ic_logo_no_chain"
		android:layout_width="wrap_content"
		android:layout_height="80dp"
		android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true"
		android:padding="5dp"
		android:id="@+id/app_icon_imageView" />
	<include
		android:id="@+id/warningBar"
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		layout="@layout/warningbar"
		android:layout_below="@id/app_icon_imageView" />
	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_below="@id/warningBar"
		android:id="@+id/proximity_status_page_counter_on_scrollView">
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/proximity_status_page_scrollView_relativeLayout">
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/proximity_status_page_counters_relativeLayout">
				<RelativeLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_margin="16dp"
					android:id="@+id/proximity_status_page_counter_on_relativeLayout"
					style="@style/CounterBackground">
					<TextView
						android:text="Number of contacts"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="21dp"
						android:layout_marginRight="21dp"
						android:layout_marginLeft="21dp"
						android:textAlignment="center"
						android:layout_centerHorizontal="true"
						android:id="@+id/number_counter_text_textView"
						style="@style/contactsTodayText" />
					<TextView
						android:text="0"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerHorizontal="true"
						android:layout_below="@+id/number_counter_text_textView"
						android:layout_marginTop="10dp"
						android:layout_marginBottom="23dp"
						android:layout_marginRight="16dp"
						android:id="@+id/number_counter_textView"
						style="@style/counterNumber" />
					<ImageView
						android:src="@drawable/ic_person"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_toRightOf="@+id/number_counter_textView"
						android:layout_alignBottom="@+id/number_counter_textView"
						android:id="@+id/number_counter_imageView" />
					<TextView
						android:text="Explanation"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerHorizontal="true"
						android:layout_marginBottom="24dp"
						android:layout_below="@+id/number_counter_textView"
						android:id="@+id/counter_explained_text_textView"
						style="@style/CounterExplainText" />
					<ImageButton
						android:src="@drawable/ic_information"
						android:layout_width="24dp"
						android:layout_height="24dp"
						android:scaleType="fitCenter"
						android:layout_marginLeft="10dp"
						android:layout_toRightOf="@+id/counter_explained_text_textView"
						android:layout_below="@+id/number_counter_textView"
						android:background="@null"
						android:id="@+id/number_counter_text_help_imageButton" />
					<View
						android:layout_width="match_parent"
						android:layout_marginTop="13dp"
						android:layout_marginRight="13dp"
						android:layout_marginLeft="13dp"
						android:layout_height="1dp"
						android:layout_below="@+id/number_counter_text_help_imageButton"
						android:background="@color/dividerWhite"
						android:id="@+id/vertical_devider" />
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingBottom="10dp"
						android:orientation="horizontal"
						android:layout_below="@+id/vertical_devider"
						android:id="@+id/average_contacts_relativeLayout"
						style="@style/CounterBackground">
						<include
							layout="@layout/proximity_sublayout"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_marginLeft="15dp"
							android:layout_marginRight="15dp"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="10dp"
							android:id="@+id/userData"
							/>
						<View
							android:layout_width="1dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="9dp"
							android:layout_marginBottom="9dp"
							android:minHeight="100dp"
							android:layout_gravity="center"
							android:background="@color/dividerWhite"
							android:id="@+id/horizontal_devider" />
						<include
							layout="@layout/proximity_sublayout"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_marginLeft="15dp"
							android:layout_marginRight="15dp"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="10dp"
							android:id="@+id/allUsersData"/>
					</LinearLayout>
				</RelativeLayout>
				<RelativeLayout
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_margin="16dp"
					android:id="@+id/proximity_status_page_counter_off_relativeLayout">
					<TextView
						android:text="Counter off textview"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="21dp"
						android:layout_marginRight="21dp"
						android:layout_marginLeft="21dp"
						android:textAlignment="center"
						android:layout_centerInParent="true"
						android:id="@+id/counter_off_text_textView"
						style="@style/contactsTodayText" />
				</RelativeLayout>
			</RelativeLayout>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_below="@+id/proximity_status_page_counters_relativeLayout"
				android:id="@+id/relativeLayout1">
				<Button
					android:text="On"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_marginBottom="24dp"
					android:layout_marginTop="29dp"
					android:id="@+id/on_off_button"
					style="@style/OnOffButton" />
				<TextView
					android:text="Unsupported"
					android:textAlignment="center"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_marginBottom="24dp"
					android:layout_margin="16dp"
					android:layout_below="@+id/on_off_button"
					android:id="@+id/unsupported_Transmit_text_textView"
					style="@style/UnsupportedText" />
				<TextView
					android:text="Last updated"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAlignment="center"
					android:layout_centerHorizontal="true"
					android:layout_marginBottom="10dp"
					android:layout_below="@+id/unsupported_Transmit_text_textView"
					android:id="@+id/last_updated_textView"
					style="@style/LastUpdatedText" />
			</RelativeLayout>
		</RelativeLayout>
	</ScrollView>
</RelativeLayout>